<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="Tugas1"/>
        <attribute name="authors" value="biant"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-10-08 10:33:03 PM"/>
